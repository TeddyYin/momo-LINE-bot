<div>
  <h1><%= @channel.channel_name %></h1>
  <table class="table table-hover">
    <thead>
      <tr>
        <td>#</td>
        <td>Type</td>
        <td>Content</td>
        <td>created_at</td>
      </tr>
    </thead>
    <tbody>
      <% @channel_chat.each do |content| %>
        <tr>
          <td>#</td>
          <td><%= content.model_name.name %></td>
          <td><%= content.text %></td>
          <td><%= content.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
